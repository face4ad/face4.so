%h2 Sign up
  Sina id:#{session[:sina_id]}
  %p
    Sina name:#{session[:sina_name]}
    
    \#{link_to("Error, please login by sina", sync_new_path(:sina) ) if session[:sina_id].nil? or session[:sina_name].nil?}
  %p
    = form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
      = devise_error_messages!
      %div
        = f.label :email
        %br/
        = f.email_field :email
      %div
        = f.label :name
        %br/
        = f.text_field :name, :value => session[:sina_name]
      %div
        = f.label :password
        %br/
        = f.password_field :password
      %div
        Skipped:#{f.label :password_confirmation}
        %br/
      %div= f.submit "Sign up"
    = render :partial => "devise/shared/links"
